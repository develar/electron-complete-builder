{
  "name": "electron-complete-builder",
  "description": "Complete solution to build ready for distribution and 'auto update' installers of your Electron App",
  "version": "0.5.4",
  "license": "MIT",
  "bin": {
    "install-app-deps": "./out/install-app-deps.js",
    "build": "./out/build-cli.js"
  },
  "scripts": {
    "compile": "tsconfig -i 2 && ts-babel",
    "lint": "tslint src/*",
    "pretest": "npm run lint",
    "test": "ava",
    "release": "npm prune && npm run compile && npm run test && npm publish",
    "preversion": "npm prune && npm run compile && npm run test"
  },
  "dependencies": {
    "bluebird": "^3.2.2",
    "command-line-args": "^2.1.4",
    "electron-builder-tf": "^2.7.3-beta.0",
    "electron-packager-tf": "^5.2.1-beta.2",
    "electron-winstaller-temp-fork": "^0.1.0",
    "hosted-git-info": "^2.1.4",
    "json-parse-helpfulerror": "^1.0.3",
    "mime": "^1.3.4",
    "progress": "^1.1.8",
    "progress-stream": "^1.2.0",
    "rimraf": "^2.5.1",
    "source-map-support": "^0.4.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.3.5"
  },
  "devDependencies": {
    "fs-extra": "^0.26.5",
    "ava-tf": "^0.11.2-beta.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "plist": "^1.2.0",
    "should": "^8.2.2",
    "tmp": "^0.0.28",
    "ts-babel": "^0.3.0",
    "tsconfig-glob": "^0.4.1",
    "tslint": "^3.3.0",
    "typescript": "^1.8.0-beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/electron-complete-builder.git"
  },
  "bugs": "https://github.com/develar/electron-complete-builder/issues",
  "engines": {
    "node": ">=0.4.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-parameters"
    ]
  },
  "ava": {
    "verbose": true
  },
  "keywords": [
    "electron",
    "builder",
    "build",
    "installer",
    "install",
    "packager",
    "pack",
    "nsis",
    "appdmg",
    "app",
    "dmg",
    "msi",
    "exe",
    "setup",
    "Windows",
    "OS X",
    "Mac",
    "sign"
  ]
}
