{
  "name": "electron-complete-builder",
  "description": "Complete solution to build ready for distribution and 'auto update' installers of your Electron App",
  "version": "0.5.0-beta.1",
  "license": "MIT",
  "bin": {
    "install-app-deps": "./out/install-app-deps.js",
    "build": "./out/build-cli.js"
  },
  "scripts": {
    "compile": "tsconfig -i 2 && ts-babel",
    "lint": "tslint src/*",
    "pretest": "npm run lint",
    "test": "mocha",
    "prepublish": "npm prune && npm run compile && npm run test",
    "preversion": "npm prune && npm run compile && npm run test"
  },
  "dependencies": {
    "bluebird": "^3.1.4",
    "command-line-args": "^2.1.4",
    "del": "^2.2.0",
    "electron-builder": "^2.6.0",
    "electron-installer-squirrel-windows": "^1.2.2",
    "electron-packager": "^5.2.1",
    "hosted-git-info": "^2.1.4",
    "mime": "^1.3.4",
    "progress": "^1.1.8",
    "progress-stream": "^1.2.0",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-parameters": "^6.4.5",
    "mocha": "^2.3.4",
    "plist": "^1.2.0",
    "rimraf": "^2.5.1",
    "should": "^8.2.0",
    "ts-babel": "^0.3.0",
    "tsconfig-glob": "^0.4.0",
    "tslint": "^3.3.0",
    "typescript": "^1.8.0-dev.20160121"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/electron-complete-builder.git"
  },
  "bugs": "https://github.com/develar/electron-complete-builder/issues",
  "engines": {
    "node": ">=0.4.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-parameters"
    ]
  },
  "keywords": [
    "electron",
    "builder",
    "build",
    "installer",
    "install",
    "packager",
    "pack",
    "nsis",
    "appdmg",
    "app",
    "dmg",
    "msi",
    "exe",
    "setup",
    "Windows",
    "OS X",
    "Mac",
    "sign"
  ]
}
